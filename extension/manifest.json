{
  "name": "Twitter for GitHub",
  "version": "1.0.1",
  "description": "Twitter handles for GitHub",
  "homepage_url": "https://github.com/bevacqua/twitter-for-github",
  "manifest_version": 2,
  "minimum_chrome_version": "48",
  "permissions": [
    "tabs",
    "https://github.com/*"
  ],
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "vendor/sprint.min.js",
        "content.js"
      ]
    }
  ]
}
